root = true

[*]
indent_style = tab
tab_width = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.java]
ij_continuation_indent_size = 8
ij_java_imports_layout = $*, |, java.**, |, javax.**, |, *, |, net.minecraft.**, |, net.fabricmc.**|, dev.spiritstudios.**
ij_java_class_count_to_use_import_on_demand = 999